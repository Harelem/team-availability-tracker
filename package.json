{
  "name": "team-availability-tracker",
  "version": "2.2.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "dev:clean": "rm -rf .next && rm -rf node_modules/.cache && next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "clean": "rm -rf .next && rm -rf node_modules/.cache",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:audit": "jest __tests__/audit --coverage --verbose",
    "test:accessibility-full": "jest __tests__/accessibility --coverage --verbose",
    "test:integration": "jest __tests__/audit/*Integration.test.ts --verbose",
    "audit:pre-production": "node scripts/run-pre-production-audit.js",
    "audit:performance": "jest __tests__/audit/performance.test.ts --verbose",
    "audit:security": "jest __tests__/audit/security.test.tsx --verbose",
    "audit:data-accuracy": "jest __tests__/audit/dataAccuracy.test.ts --verbose",
    "audit:ui-patterns": "jest __tests__/audit/uiPatterns.test.ts --verbose",
    "test:data-integrity": "jest __tests__/integrity --coverage --verbose",
    "test:data-integrity-full": "ts-node __tests__/runDataIntegrityTests.ts",
    "test:persistence": "jest __tests__/integrity/dataPersistence.test.ts --verbose",
    "test:validation": "jest __tests__/integrity/fieldValidation.test.ts --verbose",
    "test:calculations": "jest __tests__/integrity/calculationVerification.test.ts --verbose",
    "test:edge-cases": "jest __tests__/integrity/edgeCaseTesting.test.ts --verbose",
    "verify:data-integrity": "node __tests__/standalone-integrity-test.js",
    "test:compatibility": "jest __tests__/compatibility --coverage --verbose",
    "test:mobile": "jest __tests__/mobile --coverage --verbose",
    "test:devices": "jest __tests__/devices --coverage --verbose",
    "test:performance": "jest __tests__/performance --coverage --verbose",
    "test:cross-browser": "npm run test:compatibility && npm run test:mobile && npm run test:devices && npm run test:performance",
    "test:e2e": "playwright test",
    "test:e2e:headed": "playwright test --headed",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:debug": "playwright test --debug",
    "test:e2e:report": "playwright show-report",
    "test:visual": "playwright test --grep='visual'",
    "test:browser:chrome": "playwright test --project=chromium-desktop",
    "test:browser:firefox": "playwright test --project=firefox-desktop",
    "test:browser:safari": "playwright test --project=webkit-desktop",
    "test:mobile:ios": "playwright test --project=iphone-14-pro",
    "test:mobile:android": "playwright test --project=pixel-5",
    "test:tablet": "playwright test --project=ipad",
    "generate:icons": "node scripts/generate-pwa-icons.js",
    "generate:compatibility-report": "node scripts/generate-compatibility-report.js",
    "pwa:audit": "npx lighthouse --view --preset=pwa",
    "semgrep": "uvx semgrep --config=.semgrep.yml .",
    "test:regression": "jest __tests__/regression --coverage --verbose --bail",
    "test:regression:data-integrity": "jest __tests__/regression/data-integrity.test.ts --verbose",
    "test:regression:permissions": "jest __tests__/regression/permissions.test.ts --verbose",
    "test:regression:user-journeys": "jest __tests__/regression/user-journeys.test.ts --verbose",
    "test:regression:performance": "jest __tests__/regression/performance.test.ts --verbose",
    "test:regression:mobile": "jest __tests__/regression/mobile.test.ts --verbose",
    "test:regression:error-recovery": "jest __tests__/regression/error-recovery.test.ts --verbose",
    "test:regression:all": "npm run test:regression:data-integrity && npm run test:regression:permissions && npm run test:regression:user-journeys && npm run test:regression:performance && npm run test:regression:mobile && npm run test:regression:error-recovery",
    "test:agent:bug-fix": "jest __tests__/agent-tests/bug-fix.test.ts --verbose",
    "test:agent:database-auditor": "jest __tests__/agent-tests/database-auditor.test.ts --verbose",
    "test:agent:performance": "jest __tests__/agent-tests/performance.test.ts --verbose",
    "test:agent:mobile": "jest __tests__/agent-tests/mobile.test.ts --verbose",
    "test:agent:ui-polish": "jest __tests__/agent-tests/ui-polish.test.ts --verbose",
    "test:agent:coverage": "jest __tests__/agent-tests/coverage.test.ts --verbose",
    "test:v21:critical": "npm run test:regression:data-integrity && npm run test:regression:permissions && npm run test:regression:error-recovery",
    "test:v21:high": "npm run test:regression:performance && npm run test:regression:mobile && npm run test:regression:user-journeys",
    "test:v21:all": "npm run test:v21:critical && npm run test:v21:high",
    "v21:readiness-check": "node scripts/v21-readiness-check.js",
    "v21:score": "node scripts/calculate-regression-score.js",
    "semgrep:security": "uvx semgrep --config=p/security-audit --config=p/owasp-top-ten --config=p/cwe-top-25 .",
    "semgrep:quality": "uvx semgrep --config=p/eslint --config=p/react-best-practices --config=p/typescript-best-practices .",
    "semgrep:secrets": "uvx semgrep --config=p/secrets .",
    "semgrep:react": "uvx semgrep --config=p/react --config=p/nextjs .",
    "semgrep:custom": "uvx semgrep --config=semgrep/custom-rules/ .",
    "semgrep:ci": "uvx semgrep --config=p/security-audit --config=p/owasp-top-ten --config=p/cwe-top-25 --config=semgrep/custom-rules/ --error --quiet --output=semgrep/reports/semgrep-results.json --json .",
    "audit:security-enhanced": "npm run audit:security && npm run semgrep:security",
    "db:health": "node scripts/startup-validation.js health",
    "db:validate": "node scripts/validate-database-fix.js",
    "db:verify": "node scripts/verify-database-functions.js",
    "startup:validate": "node scripts/startup-validation.js startup",
    "ci:validate": "node scripts/startup-validation.js ci"
  },
  "dependencies": {
    "@axe-core/react": "^4.10.2",
    "@supabase/supabase-js": "^2.50.3",
    "@tailwindcss/forms": "^0.5.10",
    "@use-gesture/react": "^10.3.1",
    "clsx": "^2.1.1",
    "date-fns": "^4.1.0",
    "dotenv": "^17.1.0",
    "focus-trap-react": "^11.0.4",
    "lucide-react": "^0.525.0",
    "next": "15.3.5",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "recharts": "^3.1.0",
    "tailwind-merge": "^3.3.1",
    "web-push": "^3.6.7",
    "workbox-webpack-plugin": "^7.3.0",
    "xlsx": "^0.18.5",
    "zustand": "^5.0.7"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@playwright/test": "^1.40.0",
    "@tailwindcss/postcss": "^4",
    "@testing-library/jest-dom": "^6.6.4",
    "@testing-library/react": "^16.3.0",
    "@types/jest": "^30.0.0",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@types/web-push": "^3.6.4",
    "autoprefixer": "^10.4.20",
    "critters": "^0.0.23",
    "eslint": "^9",
    "eslint-config-next": "15.3.5",
    "jest": "^30.0.5",
    "jest-environment-jsdom": "^30.0.5",
    "tailwindcss": "^4",
    "ts-jest": "^29.4.0",
    "typescript": "^5"
  }
}
